{
	"name": "Selenized Light Github",
	"$schema": "vscode://schemas/color-theme",
	"type": "light",
	"colors": {
		"activityBar.background": "#ddd6c1",
		"activityBar.foreground": "#584c27",
		"activityBarBadge.background": "#b58900",
		"badge.background": "#b58900aa",
		"button.background": "#ac9d57",
		"debugExceptionWidget.background": "#ddd6c1",
		"debugExceptionWidget.border": "#ab395b",
		"debugToolBar.background": "#ddd6c1",
		"dropdown.background": "#ece3cc",
		"dropdown.border": "#d3af86",
		"editor.background": "#fbf3db",
		"editor.foreground": "#53676d",
		"editor.lineHighlightBackground": "#ece3cc",
		"editor.selectionBackground": "#ece3cc",
		"editorCursor.foreground": "#53676d",
		"editorGroup.border": "#ddd6c1",
		"editorGroup.dropBackground": "#ddd6c1aa",
		"editorGroupHeader.tabsBackground": "#d9d2c2",
		"editorHoverWidget.background": "#ece3cc",
		"editorLineNumber.activeForeground": "#909995",
		"editorLineNumber.foreground": "#909995",
		"editorLineNumber.dimmedForeground": "#909995",
		"editorWhitespace.foreground": "#3a4d5380",
		"editorWidget.background": "#ece3cc",
		"extensionButton.prominentBackground": "#b58900",
		"extensionButton.prominentHoverBackground": "#584c27aa",
		"focusBorder": "#b49471",
		"input.background": "#ddd6c1",
		"input.foreground": "#3a4d53",
		"input.placeholderForeground": "#3a4d53aa",
		"inputOption.activeBorder": "#d3af86",
		"list.activeSelectionBackground": "#dfca88",
		"list.activeSelectionForeground": "#6c6c6c",
		"list.highlightForeground": "#b58900",
		"list.hoverBackground": "#dfca8844",
		"list.inactiveSelectionBackground": "#d1cbb8",
		"minimap.selectionHighlight": "#ece3cc",
		"notebook.cellEditorBackground": "#f7f0e0",
		"panel.border": "#ddd6c1",
		"peekView.border": "#b58900",
		"peekViewEditor.background": "#fffbf2",
		"peekViewEditor.matchHighlightBackground": "#7744aa40",
		"peekViewResult.background": "#ece3cc",
		"peekViewTitle.background": "#ece3cc",
		"pickerGroup.border": "#2aa19899",
		"pickerGroup.foreground": "#2aa19899",
		"ports.iconRunningProcessForeground": "#2aa19899",
		"progressBar.background": "#b58900",
		"quickInputList.focusBackground": "#dfca8866",
		"selection.background": "#878b9180",
		"sideBar.background": "#ece3cc",
		"sideBarTitle.foreground": "#3a4d53",
		"statusBar.background": "#ece3cc",
		"statusBar.debuggingBackground": "#ece3cc",
		"statusBar.foreground": "#3a4d53",
		"statusBar.noFolderBackground": "#ece3cc",
		"statusBarItem.prominentBackground": "#ddd6c1",
		"statusBarItem.prominentHoverBackground": "#ddd6c199",
		"statusBarItem.remoteBackground": "#ac9d57",
		"tab.activeBackground": "#fbf3db",
		"tab.activeModifiedBorder": "#cb4b16",
		"tab.border": "#ddd6c1",
		"tab.inactiveBackground": "#d3cbb7",
		"tab.inactiveForeground": "#3a4d53",
		"tab.lastPinnedBorder": "#fbf3db",
		"terminal.ansiBlack": "#3a4d53",
		"terminal.ansiBlue": "#0072d4",
		"terminal.ansiBrightBlack": "#3a4d53",
		"terminal.ansiBrightBlue": "#006dce",
		"terminal.ansiBrightCyan": "#00978a",
		"terminal.ansiBrightGreen": "#428b00",
		"terminal.ansiBrightMagenta": "#c44392",
		"terminal.ansiBrightRed": "#d2212d",
		"terminal.ansiBrightWhite": "#fbf3db",
		"terminal.ansiBrightYellow": "#a78300",
		"terminal.ansiCyan": "#009c8f",
		"terminal.ansiGreen": "#489100",
		"terminal.ansiMagenta": "#ca4898",
		"terminal.ansiRed": "#d2212d",
		"terminal.ansiWhite": "#ece3cc",
		"terminal.ansiYellow": "#ad8900",
		"terminal.background": "#fbf3db",
		"titleBar.activeBackground": "#ece3cc",
		"walkThrough.embeddedEditorBackground": "#00000014",
		"list.warningForeground": "#53676d",
		"editor.findMatchHighlightForeground": "#fbf3db"
	},
	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#909995"
			}
		},
		{
			"scope": [
				"constant",
				"entity.name.constant",
				"variable.other.constant",
				"variable.other.enummember",
				"variable.language"
			],
			"settings": {
				"foreground": "#006dce"
			}
		},
		{
			"scope": [
				"entity",
				"entity.name"
			],
			"settings": {
				"foreground": "#8762c6"
			}
		},
		{
			"scope": "variable.parameter.function",
			"settings": {
				"foreground": "#3a4d53"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#489100"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#d2212d"
			}
		},
		{
			"scope": [
				"storage",
				"storage.type"
			],
			"settings": {
				"foreground": "#d2212d"
			}
		},
		{
			"scope": [
				"storage.modifier.package",
				"storage.modifier.import",
				"storage.type.java"
			],
			"settings": {
				"foreground": "#3a4d53"
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string",
				"string punctuation.section.embedded source"
			],
			"settings": {
				"foreground": "#0072d4"
			}
		},
		{
			"scope": "support",
			"settings": {
				"foreground": "#0072d4"
			}
		},
		{
			"scope": "meta.property-name",
			"settings": {
				"foreground": "#0072d4"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#c25d1e"
			}
		},
		{
			"scope": "variable.other",
			"settings": {
				"foreground": "#3a4d53"
			}
		},
		{
			"scope": "invalid.broken",
			"settings": {
				"foreground": "#d2212d",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#d2212d",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#d2212d",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "invalid.unimplemented",
			"settings": {
				"foreground": "#d2212d",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "carriage-return",
			"settings": {
				"foreground": "#fbf3db",
				// "background": "#d2212d",
				"fontStyle": "italic underline"
			}
		},
		{
			"scope": "message.error",
			"settings": {
				"foreground": "#d2212d"
			}
		},
		{
			"scope": "string variable",
			"settings": {
				"foreground": "#0072d4"
			}
		},
		{
			"scope": [
				"source.regexp",
				"string.regexp"
			],
			"settings": {
				"foreground": "#3a4d53"
			}
		},
		{
			"scope": [
				"string.regexp.character-class",
				"string.regexp constant.character.escape",
				"string.regexp source.ruby.embedded",
				"string.regexp string.regexp.arbitrary-repitition"
			],
			"settings": {
				"foreground": "#3a4d53"
			}
		},
		{
			"scope": "string.regexp constant.character.escape",
			"settings": {
				"foreground": "#489100",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "support.constant",
			"settings": {
				"foreground": "#0072d4"
			}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#0072d4"
			}
		},
		{
			"scope": "meta.module-reference",
			"settings": {
				"foreground": "#0072d4"
			}
		},
		{
			"scope": "punctuation.definition.list.begin.markdown",
			"settings": {
				"foreground": "#c25d1e"
			}
		},
		{
			"scope": [
				"markup.heading",
				"markup.heading entity.name"
			],
			"settings": {
				"foreground": "#0072d4",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.quote",
			"settings": {
				"foreground": "#489100"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"foreground": "#3a4d53",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"foreground": "#3a4d53",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"markup.underline"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": [
				"markup.strikethrough"
			],
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#0072d4"
			}
		},
		{
			"scope": [
				"markup.deleted",
				"meta.diff.header.from-file",
				"punctuation.definition.deleted"
			],
			"settings": {
				"foreground": "#d2212d"//,
				// "background": "#FFEEF0"
			}
		},
		{
			"scope": [
				"markup.inserted",
				"meta.diff.header.to-file",
				"punctuation.definition.inserted"
			],
			"settings": {
				"foreground": "#489100"//,
				// "background": "#F0FFF4"
			}
		},
		{
			"scope": [
				"markup.changed",
				"punctuation.definition.changed"
			],
			"settings": {
				"foreground": "#c25d1e",
				// "background": "#FFEBDA"
			}
		},
		{
			"scope": [
				"markup.ignored",
				"markup.untracked"
			],
			"settings": {
				"foreground": "#F6F8FA",
				// "background": "#0072d4"
			}
		},
		{
			"scope": "meta.diff.range",
			"settings": {
				"foreground": "#8762c6",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#0072d4"
			}
		},
		{
			"scope": "meta.separator",
			"settings": {
				"foreground": "#0072d4",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "meta.output",
			"settings": {
				"foreground": "#0072d4"
			}
		},
		{
			"scope": [
				"brackethighlighter.tag",
				"brackethighlighter.curly",
				"brackethighlighter.round",
				"brackethighlighter.square",
				"brackethighlighter.angle",
				"brackethighlighter.quote"
			],
			"settings": {
				"foreground": "#53676d"
			}
		},
		{
			"scope": "brackethighlighter.unmatched",
			"settings": {
				"foreground": "#d2212d"
			}
		},
		{
			"scope": [
				"constant.other.reference.link",
				"string.other.link"
			],
			"settings": {
				"foreground": "#3a4d53",
				"fontStyle": "underline"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#0072d4"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#ad8900"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#d2212d"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#ca4898"
			}
		}
	]
}
