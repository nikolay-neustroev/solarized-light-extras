{
  "name": "solarized-light-extras",
  "displayName": "Solarized Light Extras",
  "description": "A collection of themes combining Solarized and Selenized light color palettes with IntelliJ and Github syntax highlighting",
  "version": "0.0.2",
  "publisher": "NikolayNeustroev",
  "license": "MIT",
  "engines": {
    "vscode": "^1.100.0"
  },
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikolay-neustroev/solarized-light-extras"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "light",
    "solarized",
    "selenized"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Selenized Light IntelliJ",
        "uiTheme": "vs",
        "path": "./themes/selenized-light-intellij-color-theme.json"
      },
      {
        "label": "Solarized Light IntelliJ",
        "uiTheme": "vs",
        "path": "./themes/solarized-light-intellij-color-theme.json"
      },
      {
        "label": "Selenized Light Github",
        "uiTheme": "vs",
        "path": "./themes/selenized-light-github-color-theme.json"
      },
      {
        "label": "Solarized Light Github",
        "uiTheme": "vs",
        "path": "./themes/solarized-light-github-color-theme.json"
      }

    ]
  },
  "devDependencies": {
    "vsce": "^1.100.1"
  },
  "scripts": {
    "package": "mkdir -p build && vsce package -o ./build/solarized-light-extras-vscode-theme.vsix",
    "release": "vsce publish"
  }

}
